<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Create_Month__c</fullName>
    <description>Show Create Date in Year-Month Format</description>
    <externalId>false</externalId>
    <formula>TEXT(YEAR(DATEVALUE(CreatedDate)))&amp;&quot;-&quot;&amp; RIGHT(&quot;0&quot;+TEXT(MONTH(DATEVALUE(CreatedDate))),2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Create Month</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
