<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IC1_Start_Before_Finish</fullName>
    <active>true</active>
    <errorConditionFormula>Incompletion_Code_1_Start__c &gt; Incompletion_Code_1_Finish__c
||
Incompletion_Code_1_Finish__c - Incompletion_Code_1_Start__c &gt; 1</errorConditionFormula>
    <errorDisplayField>Incompletion_Code_1_Start__c</errorDisplayField>
    <errorMessage>Start Date must be set earlier than End Date and Duration no longer than 24 hours.</errorMessage>
</ValidationRule>
