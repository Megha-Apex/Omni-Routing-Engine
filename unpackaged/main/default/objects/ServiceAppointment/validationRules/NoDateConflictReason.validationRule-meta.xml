<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NoDateConflictReason</fullName>
    <active>false</active>
    <errorConditionFormula>AND
(
IF(Initial_Request_Date__c != ArrivalWindowEndTime,true,false),
ISBLANK(TEXT(Initial_Request_Date__c))
)</errorConditionFormula>
    <errorDisplayField>Initial_Request_Date__c</errorDisplayField>
    <errorMessage>Initial Request Date Is Different From Arrival Window. Date Conflict Reason Cannot Be Blank</errorMessage>
</ValidationRule>
