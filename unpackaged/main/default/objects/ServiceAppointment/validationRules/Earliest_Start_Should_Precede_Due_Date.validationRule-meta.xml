<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Earliest_Start_Should_Precede_Due_Date</fullName>
    <active>true</active>
    <errorConditionFormula>DueDate &lt; EarliestStartTime</errorConditionFormula>
    <errorMessage>The due date must be after or equal to the earliest start permitted</errorMessage>
</ValidationRule>
