<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_fill_incompletion_reason</fullName>
    <active>true</active>
    <errorConditionFormula>(ISPICKVAL(Status, &quot;Canceled&quot;)||ISPICKVAL(Status, &quot;Field Incompleted&quot;))
&amp;&amp;
ISBLANK(TEXT(Incompletion_Reason_Code__c))</errorConditionFormula>
    <errorMessage>Please fill an Incompletion Reason Code</errorMessage>
</ValidationRule>
