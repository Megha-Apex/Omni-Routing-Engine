<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Local_DueDate__c</fullName>
    <description>Show local Due Date based on Service Territory</description>
    <formula>DATE(VALUE(LEFT(TZDD__c ,4)),VALUE(MID(TZDD__c ,6,02)),VALUE(MID(TZDD__c ,9,02)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Show local Due Date based on Service Territory</inlineHelpText>
    <label>Local DueDate</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
